<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.guest.gobal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Gobal"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HotelsActivity" />
        <activity
            android:name=".FlightsActivity"
            android:label="@string/title_activity_flights"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AutosActivity"
            android:label="@string/title_activity_autos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HotelsShowActivity" />
        <activity android:name=".FlightsShowActivity"></activity>
    </application>

</manifest>